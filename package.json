{
  "name": "redux-boilerplate",
  "private": true,
  "description": "React / Redux / Redux-elm Boilerplate",
  "author": "Salsita Software (https://www.salsitasoft.com/)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:salsita/redux-boilerplate.git"
  },
  "homepage": "https://www.salsitasoft.com/",
  "scripts": {
    "postinstall": "npm run create_local_config && npm run build:be && npm run build:fe",
    "build:fe": "./node_modules/.bin/webpack --config=webpack/webpack.prod.frontend.config.babel.js",
    "build:be": "./node_modules/.bin/webpack --config=webpack/webpack.prod.backend.config.babel.js",
    "start": "node ./dist/server.js",
    "start:dev": "./node_modules/.bin/babel-node devRunner.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "test_frontend": "karma start",
    "test_backend": "mocha --reporter nyan --compilers js:babel/register $(find src/server -path '*.spec.js')",
    "test_backend_cci": "mocha --reporter mocha-junit-reporter --compilers js:babel/register $(find src/server -path '*.spec.js')",
    "tests": "npm run lint && npm run test_backend_cci && npm run test_frontend",
    "build-artifacts": "tar -czf artifacts.tgz dist node_modules package.json",
    "create_local_config": "touch src/server/config/local.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.0.1",
    "estraverse": "^4.1.1",
    "estraverse-fb": "^1.3.1",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-nyan-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-junit-reporter": "^1.9.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.9.1",
    "request": "^2.65.0",
    "single-child": "^0.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "assign-deep": "^0.4.3",
    "babel-runtime": "^6.9.0",
    "express": "^4.13.4",
    "history": "^2.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.1.2",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.3.1",
    "redux-elm": "^2.1.1",
    "redux-saga": "^0.10.4"
  }
}
