{
  "name": "redux-boilerplate",
  "private": true,
  "description": "React / Redux / Redux-elm Boilerplate",
  "author": "Salsita Software (https://www.salsitasoft.com/)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:salsita/redux-boilerplate.git"
  },
  "homepage": "https://www.salsitasoft.com/",
  "scripts": {
    "postinstall": "npm run create_local_config && npm run build_backend && npm run build_frontend",
    "build_frontend": "node ./node_modules/webpack/bin/webpack.js -p --config=webpack/webpack.prod.frontend.config.js",
    "build_backend": "node ./node_modules/webpack/bin/webpack.js -p --config=webpack/webpack.prod.backend.config.js",
    "start": "node ./dist/server.js",
    "start:dev": "./node_modules/.bin/babel-node devRunner.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "test_frontend": "karma start",
    "test_backend": "mocha --reporter nyan --compilers js:babel/register $(find src/server -path '*.spec.js')",
    "test_backend_cci": "mocha --reporter mocha-junit-reporter --compilers js:babel/register $(find src/server -path '*.spec.js')",
    "tests": "npm run lint && npm run test_backend_cci && npm run test_frontend",
    "build-artifacts": "tar -czf artifacts.tgz dist node_modules package.json",
    "create_local_config": "touch src/server/config/local.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^5.4.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.1.0",
    "estraverse": "^4.1.1",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "^0.1.1",
    "karma-junit-reporter": "^0.3.8",
    "karma-mocha": "^0.2.1",
    "karma-nyan-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-junit-reporter": "^1.9.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.7",
    "request": "^2.65.0",
    "single-child": "^0.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "assign-deep": "^0.4.3",
    "babel-runtime": "^5.8.34",
    "bluebird": "^3.3.3",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "history": "1.17.0",
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.7",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.1.2",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-side-effects": "^0.2.4",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.7.2",
    "superagent-bluebird-promise": "^3.0.0"
  }
}
